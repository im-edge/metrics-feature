{
  "name": "imedge/metrics-feature",
  "type": "project",
  "config": {
    "platform": {
      "php": "8.1.18"
    },
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IMEdge\\MetricsFeature\\": "src"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "gipfl/linux-health": ">=0.2",
    "imedge/config": ">=0.1",
    "imedge/icinga-perf-data": ">=0.5",
    "imedge/json": ">=0.1",
    "imedge/log": ">=0.7",
    "imedge/node-provided-libs": "^0.9.0",
    "imedge/rrd-graph-info": ">=0.3",
    "imedge/rrd-structure": ">=0.4",
    "imedge/rrdcached": ">=0.4",
    "ramsey/uuid": "^4"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10 | ^2",
    "phpunit/phpunit": "^10.3",
    "squizlabs/php_codesniffer": "^3.7",
    "symplify/easy-coding-standard": "^12.0"
  }
}
